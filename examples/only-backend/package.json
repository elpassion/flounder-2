{
  "name": "@fch/core",
  "version": "0.0.1",
  "engines": {
    "node": "16",
    "npm": "please-use-pnpm",
    "yarn": "please-use-pnpm",
    "pnpm": "7"
  },
  "engineStrict": true,
  "scripts": {
    "serve": "nx run-many --target=serve --projects=backend",
    "prod": "nx run-many --target=serve --projects=backend --prod",
    "build": "rimraf dist && nx run-many --target=build --projects=backend",
    "dc": "docker-compose -p fch -f",
    "dc:dependencies": "npm run dc .docker/dev/docker-compose-dependencies.yml",
    "lint": "nx affected --target=lint",
    "test": "nx affected --target=test",
    "test:e2e": "nx affected --target=test:e2e",
    "test:all": "nx run-many --target=test --all",
    "compile": "nx affected --target=tsc",
    "storybook": "nx storybook ui",
    "ci:lint": "npm run lint -- --parallel=3",
    "ci:test": "npm run test -- --parallel=3 --ci --code-coverage",
    "ci:test:e2e": "npm run test:e2e -- --parallel=3 --ci --code-coverage",
    "ci:compile": "npm run compile -- --parallel=3",
    "ci:build": "nx affected --target=build --parallel=3",
    "ci:docker:build": "nx affected --target=docker:build --parallel=3",
    "ci:docker:deploy": "nx affected --target=docker:deploy --parallel=3",
    "typeorm:migrations": "nx typeorm:migrations backend",
    "typeorm:migrations:test": "nx typeorm:migrations:test backend"
  },
  "private": true,
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/preset-typescript": "7.12.13",
    "@babel/runtime": "^7.18.3",
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@nrwl/cli": "14.4.3",
    "@nrwl/eslint-plugin-nx": "14.4.3",
    "@nrwl/jest": "14.4.3",
    "@nrwl/js": "14.4.3",
    "@nrwl/linter": "14.4.3",
    "@nrwl/nest": "14.4.3",
    "@nrwl/node": "14.4.3",
    "@nrwl/workspace": "14.4.3",
    "@svgr/webpack": "^5.4.0",
    "@swc-node/jest": "^1.4.0",
    "@types/bull": "^3.15.7",
    "@types/express": "^4.17.13",
    "@types/jest": "29.0.0",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/lodash": "^4.14.182",
    "@types/node": "18.0.0",
    "@types/passport-jwt": "^3.0.6",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "5.30.7",
    "@typescript-eslint/parser": "5.30.7",
    "babel-jest": "27.5.1",
    "babel-loader": "^8.2.3",
    "concurrently": "^7.0.0",
    "eslint": "8.15.0",
    "eslint-config-next": "12.1.6",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-jsx-a11y": "6.6.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "7.30.1",
    "eslint-plugin-react-hooks": "4.6.0",
    "husky": "^4.3.7",
    "identity-obj-proxy": "^3.0.0",
    "jest": "27.5.1",
    "jest-circus": "27.5.1",
    "jest-extended": "^2.0.0",
